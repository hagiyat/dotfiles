{
    "Keymap": {
        "C-p": "peco.SelectPrevious",
        "C-n": "peco.SelectNext",
        "C-s": "peco.ToggleSelectionAndSelectNext",
        "C-c": "peco.Cancel"
    },
    "CustomFilter": {
        "Migemo": {
            "Cmd": "migemogrep",
            "Args": [ "$QUERY" ],
            "BufferThreshold": 20
        }
    },
    "Style": {
        "Basic": ["on_default", "default"],
        "Selected": ["underline", "on_blue", "black"],
        "Matched": ["cyan", "bold"],
        "Query": ["yellow", "bold"]
    }
}
