{
  "Keymap": {
    "C-p": "peco.SelectPrevious",
    "C-n": "peco.SelectNext",
    "C-s": "peco.ToggleSelectionAndSelectNext",
    "C-/": "peco.RotateMatcher",
    "C-c": "peco.Cancel"
  },
  "CustomMatcher": {
    "C/Migemo": [
      "/usr/local/bin/cmigemo",
      "$QUERY"
    ]
  },
  "Style": {
    "Basic": ["on_default", "default"],
    "Selected": ["underline", "on_blue", "black"],
    "Query": ["yellow", "bold"]
  }
}
